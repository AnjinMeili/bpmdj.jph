CPP = g++ -O9
CC = gcc -O9 
UIC = /usr/bin/uic
MOC = moc
CFLAGS+= -D COMPILE_OSS -D COMPILE_ALSA -D QT_THREAD_SUPPORT
LDFLAGS+= -lpthread -lm -lasound -lrt -lfftw3
QT_INCLUDE_PATH=-I/usr/lib64/qt-3.3/include
QT_LIBRARY_PATH=-L/usr/lib64/qt-3.3/lib
QT_LIBS= -lqt-mt
