CPP=g++ -O3 -Wall
CC=gcc -O3 -Wall
CFLAGS+=-D COMPILE_ALSA -D COMPILE_OSS -D QT_THREAD_SUPPORT -D NO_EMPTY_ARRAYS -D COMPILE_JACK
LDFLAGS+= -lpthread -lm -lasound -lrt -lfftw3 -ljack
UIC = /usr/lib/qt3/bin/uic
MOC = /usr/lib/qt3/bin/moc
QT_INCLUDE_PATH= -I /usr/lib/qt3/include
# replace lib in the line below to the 64 bit library 
# if necessary
QT_LIBRARY_PATH= -L /usr/lib/qt3/lib
QT_LIBS= -lqt-mt
