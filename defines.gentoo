CPP = g++ -O3 -Wall
CC = gcc -O3 -Wall

CFLAGS += -D COMPILE_OSS -D COMPILE_ALSA -D QT_THREAD_SUPPORT
LDFLAGS+= -lpthread -lm -lasound -lrt -lfftw3

CP = cp
MV = mv
TAR = tar
MKDIR = mkdir
UIC = uic
MOC = moc
STRIP = strip
INSTALL = install

QT_INCLUDE_PATH= -I ${QTDIR}/include/
QT_LIBRARY_PATH= -L ${QTDIR}/lib/
QT_LIBS= -lqt 
