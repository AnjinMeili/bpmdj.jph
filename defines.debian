JOBS=1
CPP             = g++ -g -O2 -Wall
RCC             = rcc
UIC             = uic-qt4
MOC             = moc-qt4
CFLAGS         += -D COMPILE_ALSA -D COMPILE_JACK -DQT_THREAD_SUPPORT -DQT3_SUPPORT
LDFLAGS        += -pthread -lm -lasound -lrt -lfftw3 -ljack
QT_INCLUDE_PATH = -I/usr/include/qt4/ -I/usr/include/qt4/Qt/
QT_LIBRARY_PATH =
QT_LIBS         = -lQt3Support -lQtGui
BITS		= 32
